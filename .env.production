# 生产环境配置文件
# 请复制此文件为 .env 并填入真实的生产环境配置

# 服务器配置
SERVER_PORT=3000
ENVIRONMENT=production
FRONTEND_URL=https://www.ifoodme.com

# 数据库配置 (与docker-compose.yml保持一致)
BLUEPRINT_DB_HOST=postgres
BLUEPRINT_DB_PORT=5432
BLUEPRINT_DB_USERNAME=myuser
BLUEPRINT_DB_PASSWORD=mypassword
BLUEPRINT_DB_DATABASE=mydatabase
BLUEPRINT_DB_SCHEMA=public

# Google OAuth (请填入真实的生产环境密钥)
GOOGLE_CLIENT_ID=your_production_google_client_id
GOOGLE_CLIENT_SECRET=your_production_google_client_secret
GOOGLE_CALLBACK_URL=https://www.ifoodme.com/api/auth/google/callback

# JWT 密钥 (请使用强密码)
JWT_SECRET=your_production_jwt_secret_key
JWT_REFRESH_SECRET=your_production_jwt_refresh_secret_key
SESSION_SECRET=your_production_session_secret_key

# CORS 配置 - 允许的域名（逗号分隔）
ALLOWED_ORIGINS=https://www.ifoodme.com

# Cookie 域名配置（生产环境使用）
COOKIE_DOMAIN=.ifoodme.com

# 管理员账户 (请修改为真实的管理员信息)
ADMIN_EMAIL=admin@ifoodme.com
ADMIN_PASSWORD=your_strong_admin_password 