# 环境配置文件示例

# 服务器配置
SERVER_PORT=3000
ENVIRONMENT=development
FRONTEND_URL=http://localhost:3001

# 数据库配置
BLUEPRINT_DB_HOST=localhost
BLUEPRINT_DB_PORT=5432
BLUEPRINT_DB_USERNAME=your_db_username
BLUEPRINT_DB_PASSWORD=your_db_password
BLUEPRINT_DB_DATABASE=your_db_name

# Google OAuth (可选)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=https://www.ifoodme.com/api/auth/google/callback

# JWT 密钥
JWT_SECRET=your_jwt_secret_key_here
JWT_REFRESH_SECRET=your_jwt_refresh_secret_key_here
SESSION_SECRET=your_session_secret_key_here

# CORS 配置 - 允许的域名（逗号分隔）
ALLOWED_ORIGINS=http://localhost:3001,http://localhost:3000

# Cookie 域名配置（生产环境使用，如 .yourdomain.com）
COOKIE_DOMAIN=

# 管理员账户 (用于初始化)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123
